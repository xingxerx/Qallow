{
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/build/**": true
  },
  "codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.env.linux": {
    "LANG": "C.UTF-8",
    "LC_ALL": "C.UTF-8"
  }
}
