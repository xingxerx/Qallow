    Expression: "l1tex__lsu_writeback_active_mem_lgds.sum.peak_sustained * 128"
    Filter {
      MinArch: CC_90
    }
  }
  MetricDefinitions {
    Name: "derived__l1tex__lsu_writeback_bytes.sum.per_second"
    Expression: "l1tex__lsu_writeback_active.sum.per_second * 128"
    Filter {
      MaxArch: CC_72
