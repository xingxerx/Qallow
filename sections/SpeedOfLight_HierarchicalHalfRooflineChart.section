   Label: "L2 peak writeback cycles"
   Name: "lts__lts2xbar_cycles_active.sum.peak_sustained"
 }
 Metrics {
   Label: "L2 active writeback cycles per second"
   Name: "lts__lts2xbar_cycles_active.sum.per_second"
 }
}
MetricDefinitions {
  MetricDefinitions {
    Name: "derived__smsp__sass_thread_inst_executed_op_hadd_pred_on_x2"
    Expression: "smsp__sass_thread_inst_executed_op_hadd_pred_on.sum.per_cycle_elapsed * 2"
  }
  MetricDefinitions {
    Name: "derived__smsp__sass_thread_inst_executed_op_hmul_pred_on_x2"
    Expression: "smsp__sass_thread_inst_executed_op_hmul_pred_on.sum.per_cycle_elapsed * 2"
