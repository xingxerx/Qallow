Identifier: "SpeedOfLight_HierarchicalHalfRooflineChart"
DisplayName: "GPU Speed Of Light Hierarchical Roofline Chart (Half Precision)"
Extends: "SpeedOfLight"
Description: "High-level overview of the utilization for compute and memory resources of the GPU presented as a roofline chart."
Order: 12
Sets {
  Identifier: "roofline"
}
Sets {
  Identifier: "full"
}

Filter {
  CollectionFilter {
    CollectionScopes: CollectionScope_Launch
  }
}

Metrics {
 Metrics {
   Label: "Theoretical Predicated-On HFMA Thread Instructions Executed"
   Name: "sm__sass_thread_inst_executed_op_hfma_pred_on.sum.peak_sustained"
 }
 Metrics {
   Label: "Predicated-On HADD Thread Instructions Executed Per Cycle"
   Name: "smsp__sass_thread_inst_executed_op_hadd_pred_on.sum.per_cycle_elapsed"
 }
